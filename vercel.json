{
  "version": 2,
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/states-of-matter-basics",
      "destination": "/states-of-matter-basics/index.html"
    },
    {
      "source": "/states-of-matter-basics/",
      "destination": "/states-of-matter-basics/index.html"
    },
    {
      "source": "/gas-properties",
      "destination": "/gas-properties/index.html"
    },
    {
      "source": "/gas-properties/",
      "destination": "/gas-properties/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
