{
  "version": 2,
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/states-of-matter-basics(/.*)?",
      "dest": "/states-of-matter-basics/index.html"
    },
    {
      "src": "/gas-properties(/.*)?",
      "dest": "/gas-properties/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
